            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.setting.MessagePusheActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.setting.MessagePushTimeActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".activity.setting.DevSetTFActivity"
            android:screenOrientation="portrait" />

        <activity
