            android:screenOrientation="portrait" />

        <activity
            android:name=".activity.MNPlayControlActivity"
            android:configChanges="orientation|keyboard|keyboardHidden|screenSize|layoutDirection"
            android:screenOrientation="sensor" />


        <activity
